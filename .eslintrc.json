{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "function-paren-newline": ["error", "consistent"],
    "guard-for-in": 0,
    "no-bitwise": 0,
    "no-console": 0,
    "no-continue": 1,
    "no-param-reassign": 1,
    "no-restricted-syntax": 0,
    "object-curly-newline": ["error", {
      "ObjectExpression": { "multiline": true, "consistent": true },
      "ObjectPattern": { "multiline": true, "consistent": true }
    }]
  }
}
